  474 |   await this.all.nth(5).click();
  475 |   await expect(this.must3).toContainText("כללי השתתפות ");
  476 |   const screenshotPath = path.resolve('C:\\Users\\User\\Documents\\k6try\\screenshots\\ele_actual.png');
  477 |   await this.page.screenshot({ path: screenshotPath, clip: { x: 0, y: 0, width: await this.page.evaluate(() => window.innerWidth), height: 1000 } });
  478 |
  479 |   // טען את תמונת ה-expected הקיימת
  480 |   const expectedPath = path.resolve('C:\\Users\\User\\Documents\\k6try\\screenshots\\ele.png');
  481 |   const expectedImage = PNG.sync.read(fs.readFileSync(expectedPath));
  482 |   const actualImage = PNG.sync.read(fs.readFileSync(screenshotPath));
  483 |
  484 |   const { width, height } = expectedImage;
  485 |   const diff = new PNG({ width, height });
  486 |
  487 |   // השווה בין התמונות
  488 |   const diffPixels = pixelmatch(expectedImage.data, actualImage.data, diff.data, width, height, { threshold: 0.1 });
  489 |
  490 |   // בדוק שאין הבדל (תפסיק אם יש הבדל)
  491 |   expect(diffPixels, `יש הבדל של ${diffPixels} פיקסלים בין התמונה הצפויה לפועלית`).toBe(0);
  492 | };
  493 | async compareScreenshots(start: number, end: number) {
  494 |   for (let i = start; i <= end; i++) {
  495 |     await this.all.nth(i).click();
  496 |
  497 |     const screenshotPath = path.resolve(`C:\\Users\\User\\Documents\\k6try\\screenshots\\twohalve_actual_${i}.png`);
  498 |     const expectedPath = path.resolve(`C:\\Users\\User\\Documents\\k6try\\screenshots\\twohalve_${i}.png`);
  499 |
  500 |     const width = await this.page.evaluate(() => window.innerWidth);
  501 |     const height = await this.page.evaluate(() => window.innerHeight);
  502 |     
  503 |     
  504 |     await this.page.screenshot({ path: screenshotPath, clip: { x: 0, y: 0, width, height } });
  505 |
  506 |     const expectedImage = PNG.sync.read(fs.readFileSync(expectedPath));
  507 |     const actualImage = PNG.sync.read(fs.readFileSync(screenshotPath));
  508 |
  509 |     const { width: w, height: h } = expectedImage;
  510 |     const diff = new PNG({ width: w, height: h });
  511 |
  512 |     const diffPixels = pixelmatch(expectedImage.data, actualImage.data, diff.data, w, h, { threshold: 0.1 });
  513 |
  514 |     const TOLERANCE = 100;
  515 |
  516 |     expect(diffPixels, `יש הבדל של ${diffPixels} פיקסלים בין התמונה הצפויה לפועלית במסך מספר ${i}`).toBeLessThanOrEqual(TOLERANCE);
  517 |
  518 |
  519 |     await this.page.goBack();
  520 |   }
  521 | }
  522 | async captureExpectedScreenshots(start: number, end: number) {
  523 |   for (let i = start; i <= end; i++) {
  524 |     await this.all.nth(i).click();
